{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "isSupplier": {
      "type": "boolean",
      "default": false
    },
    "name": {
      "type": "string",
      "required": true
    },
    "supplierValidator": {
      "type": "boolean",
      "default": false
    },
    "supplier": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::supplier.supplier",
      "inversedBy": "users"
    },
    "organization": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::organization.organization"
    },
    "invoiceValidator": {
      "type": "boolean",
      "default": false
    },
    "invoicePayer": {
      "type": "boolean",
      "default": false
    },
    "ticketValidatorUserId": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "ticketValidator": {
      "type": "boolean",
      "default": false
    },
    "ticketPayer": {
      "type": "boolean",
      "default": false
    },
    "ticketRegistrar": {
      "type": "boolean",
      "default": false
    },
    "ticketAdmin": {
      "type": "boolean",
      "default": false
    },
    "orderValidator": {
      "type": "boolean",
      "default": false
    },
    "purchaseManager": {
      "type": "boolean",
      "default": false
    },
    "purchaseAdmonObra": {
      "type": "boolean",
      "default": false
    },
    "foreman": {
      "type": "boolean",
      "default": false
    },
    "orgManager": {
      "type": "boolean",
      "default": false
    },
    "issueManager": {
      "type": "boolean",
      "default": false
    }
  }
}
